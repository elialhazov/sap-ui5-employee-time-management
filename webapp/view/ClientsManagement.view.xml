<mvc:View
    controllerName="employeetimemanagement.controller.ClientsManagement"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    displayBlock="true">

    <Page
        title="Clients Management"
        class="clientsPage"
        showNavButton="true"
        navButtonPress=".onNavBack">

        <content>

            <VBox class="clientsLayout">

                <VBox class="employeesPanel">

                    <Table
                        id="matrixTable"
                        class="clientsMatrixTable"
                        width="100%"
                        noDataText="No data"
                        fixedLayout="false"
                        sticky="ColumnHeaders">
                    </Table>

                </VBox>

            </VBox>

        </content>
    </Page>
</mvc:View>